{
  "name": "koa-todos-api",
  "version": "1.0.0",
  "description": "A todos api written using koa for practice",
  "homepage": "https://github.com/troylugar/koa-todos-api",
  "repository": {
    "url": "https://github.com/troylugar/koa-todos-api.git",
    "type": "git"
  },
  "main": "src/server.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --coverage",
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
    "start:staging": "NODE_ENV=staging node src/server.js",
    "start:production": "NODE_ENV=production node src/server.js"
  },
  "author": "Troy Lugar",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-pino-logger": "^3.0.0",
    "mongoose": "^5.11.11",
    "pino": "^6.10.0",
    "pino-pretty": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "mockdate": "^3.0.2",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.1"
